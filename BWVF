coroutine.wrap(DKBAKCM_fake_script)()
local function ZHKXXC_fake_script() -- FOV_Slider.LocalScript 
	local script = Instance.new('LocalScript', FOV_Slider)

	--Variables-------------------------------------------------
	local UIS = game:GetService('UserInputService')
	local Slider = script.Parent
	local InnerSliderA = script.Parent.InnerSliderA
	local InnerSliderB = script.Parent.InnerSliderB
	local SliderInfo = script.Parent.FOVInfo
	local player = game.Players.LocalPlayer
	local RUS = game:GetService('RunService')
	SliderInfo.Text = '0/10'
	--Properties------------------------------------------
	local Held = false                                                    -- Variable for if you are holding down the slider, holds information that the script would need.
	local factor = 0.1                                                   --Step is the factor
	
	--Script------------------------------------------------------
	function Round(number, factor)                            
		if factor == 0 then                                               -- IF there is no factor then it just returns the % that the innerslider covers              
			return number        
		end
		return math.floor(number/factor+0.5) * factor                     -- Returns the sum of this to be the value of Pos, would now be an increment of 'factor'  e.g. only goes up in 0.01 by rounding down anything that isnt an increment/factor of 0.01 to an increment/factor of 0.01.  
	end
	
	
	Slider.MouseButton1Down:Connect(function()                       -- When you click and are holding down the slider with LMB
		Held = true                                                       -- Makes held variable true
	end)
	InnerSliderA.MouseButton1Down:Connect(function()
		Held = true
	end)
	InnerSliderB.MouseButton1Down:Connect(function()
		Held = true
	end)
	UIS.InputEnded:Connect(function(Input, Processed)                     -- When you stop holding down the slider with LMB
		if Input.UserInputType == Enum.UserInputType.MouseButton1 then
			Held = false                                                  -- Makes held variable false
		end
	end)
	
	RUS.RenderStepped:Connect(function(delta)                            				  -- An event that fires prior to the frame being rendered
		if Held then                                                     				  -- If the slider is being held then
			local MousePos = UIS:GetMouseLocation().X                  				      -- gets the X value of where the Mouse is at on the screen
			local ISPos1 = InnerSliderA.Position                              		      -- ISPos is the InnerSlider's Position
			local ISSize1 = InnerSliderA.Size                                               -- ISSize is the InnerSlider's Size
			local ISPos2 = InnerSliderB.Position 
			local ISSize2 = InnerSliderB.Size
			local SliderPos = Slider.AbsolutePosition.X                  				  -- Gets the X value of the Slider's Position
			local SliderSize = Slider.AbsoluteSize.X                                      -- Gets the X value of the Slider's Size
	
			local Pos = Round((MousePos-SliderPos)/SliderSize, factor)                    -- '(MousePos-SliderPos)/SliderSize is the % of the slider that the inner slider covers. 1 is 100%, 0 is 0%. (With mouse it'd be the % of the way to the end of the slider),
			                                                                              -- 'Round' sends the values to be rounded in a function, where Pos would be the value that the function returns
			local Percentage = math.clamp(Pos,0,1)                                        -- For math.clamp, The first argument is the value you want to clamp, the 2nd is the minimum(smallest) value that the clamped value can be, 3rd is the maximum(biggest) value it can be
	
	
			InnerSliderA.Size = UDim2.new(Percentage, 0, ISSize1.Y.Scale, ISSize1.Y.Offset)  -- Makes the InnerSlider's X Scale according to the % it should cover
			InnerSliderB.Size = UDim2.new(Percentage, 0, ISSize2.Y.Scale, ISSize2.Y.Offset)
			SliderInfo.Text = math.floor(Percentage * 10)..'/10'                                -- Makes SliderInfo the % that the innerslider covers
			getgenv().AimbotFOV.CircleRadius =  math.floor(Percentage * 200)
		end 
	end)
	
	
	
	
	
end
coroutine.wrap(ZHKXXC_fake_script)()
local function TRJXTU_fake_script() -- InnerSliderB_3.LocalScript 
	local script = Instance.new('LocalScript', InnerSliderB_3)

	local Tick = tick()
	local renderStepped = game:GetService('RunService').RenderStepped
	local Hue = 0
	
	
	while renderStepped:Wait() do -- Service runs every time prior to a frame being rendered, Waits for the service to end before running again.
		if tick() - Tick >= (1/60) then -- If time passed since the previous loop ended in greater than or equal to 1/60 then:
			Hue = Hue + (1/400) -- Increases Hue value by 1/400 every loop
	
			if Hue >= 1 then -- If Hue reaches the max Hue value(1) then
				Hue = 0 -- Reset Hue back to 0
			end
			script.Parent.BackgroundColor3 = Color3.fromHSV(Hue, 0.8, 1) -- Changes the Color every loop - Creates the rainbow accent color.
	
			Tick = tick() -- Updates the time the previous loop ends
	
	
		end	
	end
	
	
	-- 'tick() - Tick' Creates a Sum that is the time passed since the previous loop ended
	
end
coroutine.wrap(TRJXTU_fake_script)()
local function IWWG_fake_script() -- InnerSliderB_3.Script 
	local script = Instance.new('Script', InnerSliderB_3)

	local B = script.Parent
	local A = script.Parent.Parent.InnerSliderA
	local RenderStepped = game:GetService('RunService').RenderStepped
	B.Visible = true
	A.Visible = false
	
	
	local loop = true
	while loop == true do 
		if getgenv().Rainbow_On == true and getgenv().Rainbow_Off == false then
			B.Visible = true
			A.Visible = false
		end
		if getgenv().Rainbow_On == false and getgenv().Rainbow_Off == true then
			B.Visible = false
			A.Visible = true
		end
		wait()
	end
	
end
coroutine.wrap(IWWG_fake_script)()
local function GZBA_fake_script() -- IgnoreFOVToggle.LocalScript 
	local script = Instance.new('LocalScript', IgnoreFOVToggle)

	local Toggle = false
	local Tick = tick()
	local Hue = 0
	
	script.Parent.MouseButton1Click:Connect(function()
		if Toggle == false then
			Toggle = true
			if getgenv().Rainbow_On == true and getgenv().Rainbow_Off == false then
	
			elseif getgenv().Rainbow_On == false and getgenv().Rainbow_Off == true then
	
				script.Parent.BackgroundColor3 = getgenv().AccentColor
			end
	
			--getgenv().Aimbot.AimbotEnabled = true
		elseif Toggle == true then
	
			Toggle = false
			script.Parent.BackgroundColor3 = getgenv().MainColor
			--getgenv().Aimbot.AimbotEnabled = false
	
		end
	end)
	
	while game:GetService('RunService').RenderStepped:Wait() do
	
		if tick() - Tick >= (1/60) then 
			Hue = Hue + (1/400)
	
			if Hue >= 1 then
				Hue = 0
			end
	
			if Toggle == true and getgenv().Rainbow_On == true and getgenv().Rainbow_Off == false then
	
				script.Parent.BackgroundColor3 = Color3.fromHSV(Hue, 0.8, 1) 
				
			elseif Toggle == true and getgenv().Rainbow_On == false and getgenv().Rainbow_Off == true then
	
				script.Parent.BackgroundColor3 = getgenv().AccentColor
				
			end
	
			Tick = tick() 
	
		end	
	end
end
