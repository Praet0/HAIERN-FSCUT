coroutine.wrap(RYFRXZS_fake_script)()
local function ZXVM_fake_script() -- ALed1.Script 
	local script = Instance.new('Script', ALed1)

	while true do
		if getgenv().Rainbow_On == true and getgenv().Rainbow_Off == false then
			script.Parent.Visible = false
			script.Parent.Parent["Led-1"].Visible = true
		elseif getgenv().Rainbow_Off == true and getgenv().Rainbow_On == false then
			script.Parent.Visible = true
			script.Parent.Parent["Led-1"].Visible = false
		end
		wait()
	end
	
end
