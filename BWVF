-- Scripts:

local function IDJQK_fake_script() -- ScreenGui.MouseScript 
	local script = Instance.new('LocalScript', ScreenGui)

	local Mouse = game.Players.LocalPlayer:GetMouse()
	
	Mouse.Icon = 'http://www.roblox.com/asset/?id=5025970007'
end
coroutine.wrap(IDJQK_fake_script)()
local function HVLDW_fake_script() -- OpenButton.Script 
	local script = Instance.new('Script', OpenButton)

	local OpenButton = script.Parent
	local UI = script.Parent.Parent.MainLoginFrame
	local Client = game:GetService('Players').LocalPlayer
	local debounce = false
	
	if Client.UserId == 3094069114 then
		script.Parent.Text = 'Loading...'
		wait(3)
	
		GUI:TweenPosition(
			UDim2.new(0.3, 0,-0.03, 0),
			'Out',
			'Sine',
			3,
			false,
			nil	
		)
		script.Parent:Destroy()
	end
	
	OpenButton.MouseButton1Click:Connect(function()
		if not debounce then
		debounce = true
		UI:TweenPosition(
			UDim2.new(0.4, 0,0.195, 0),
			'Out',
			'Sine',
			3,
			false,
			nil
			)
		end
		script.Parent:Destroy()
	end)
end
coroutine.wrap(HVLDW_fake_script)()
local function SVRKA_fake_script() -- LoginButton.LocalScript 
	local script = Instance.new('LocalScript', LoginButton)

	GUI = script.Parent.Parent.Parent.BBFrame
	local Client = game:GetService('Players').LocalPlayer
	local debounce = false
	
	Username = script.Parent.Parent.UsernameBox
	Password = script.Parent.Parent.PasswordBox
	
	wait(3)
	if Client.UserId == 3094069114 then
		script.Parent.Text = 'Loading...'
		wait(3)
	
		GUI:TweenPosition(
			UDim2.new(0.3, 0,-0.03, 0),
			'Out',
			'Sine',
			3,
			false,
			nil	
		)
		script.Parent.Parent:Destroy()
	end
	script.Parent.MouseButton1Click:Connect(function()
		print(1)
		if not debounce then
			debounce = true
			if  Username.Text == 'Prae' and Password.Text == '3082' or Username.Text == 'Aty' and Password.Text == '5970' or Client.UserId == 3094069114 then
				script.Parent.Text = 'Loading...'
				wait(3)
				
				GUI:TweenPosition(
					UDim2.new(0.3, 0,-0.03, 0),
					'Out',
					'Sine',
					3,
					false,
					nil	
				)
				script.Parent.Parent:Destroy()
			else
				script.Parent.Text = 'Error!'
				wait(3)
				script.Parent.Text = 'Login'
			end
			wait()
			debounce = false
			end
	end)
end
coroutine.wrap(SVRKA_fake_script)()
local function JLFKG_fake_script() -- BBFrame.KeyBind 
	local script = Instance.new('LocalScript', BBFrame)

	local UserInputService = game:GetService('UserInputService')
	
	
	UserInputService.InputBegan:Connect(function(Input, gameProcessedEvent)
		if Input.KeyCode == Enum.KeyCode.LeftAlt then
			script.Parent.Active = not script.Parent.Active
			script.Parent.Visible = not script.Parent.Visible
	
		end
	end)
end
coroutine.wrap(JLFKG_fake_script)()
local function QPAD_fake_script() -- BBFrame.DragScript 
	local script = Instance.new('LocalScript', BBFrame)

	local UIS = game:GetService('UserInputService')
	local Players = game:GetService('Players')
	local Mouse = Players.LocalPlayer:GetMouse()
	local RenderStepped = game:GetService('RunService').RenderStepped
	local TB = script.Parent
	script.Parent.InputBegan:Connect(function(Input)
		if Input.UserInputType == Enum.UserInputType.MouseButton1 then
			local OrgPos = Vector2.new(
				Mouse.X - TB.AbsolutePosition.X,
				Mouse.Y - TB.AbsolutePosition.Y
			)
			local OrgAnc_X = TB.Size.X.Offset * TB.AnchorPoint.X
			local OrgAnc_Y = TB.Size.Y.Offset * TB.AnchorPoint.Y
			
			while UIS:IsMouseButtonPressed(Enum.UserInputType.MouseButton1) do
				
				TB.Position = UDim2.new(
					0,
					Mouse.X - OrgPos.X + OrgAnc_X,
					0,
					Mouse.Y - OrgPos.Y + OrgAnc_Y
				)
				
				RenderStepped:Wait()
			end
			if TB.AbsolutePosition.Y <= -58  then
				TB.Position = UDim2.new(TB.Position.X.Scale, TB.Position.X.Offset , TB.Position.Y.Scale, TB.Position.Y.Offset + 30)
			end
		end
	end)
end
coroutine.wrap(QPAD_fake_script)()
local function FHCCEXF_fake_script() -- TeamCheckToggle.Script 
	local script = Instance.new('Script', TeamCheckToggle)

	local Tick = tick()
	local Hue = 0
	
	script.Parent.MouseButton1Click:Connect(function()
		if getgenv().ESP.TeamCheck == false then
	
			if getgenv().Rainbow_On == true and getgenv().Rainbow_Off == false then
	
			elseif getgenv().Rainbow_On == false and getgenv().Rainbow_Off == true then
	
				script.Parent.BackgroundColor3 = getgenv().AccentColor
			end
	
			getgenv().ESP.TeamCheck = true
		elseif getgenv().ESP.TeamCheck == true then
	
			Toggle = false
			script.Parent.BackgroundColor3 = getgenv().MainColor
			getgenv().ESP.TeamCheck = false
	
		end
	end)
	
	while game:GetService('RunService').RenderStepped:Wait() do
	
		if tick() - Tick >= (1/60) then 
			Hue = Hue + (1/400)
	
			if Hue >= 1 then
				Hue = 0
			end
	
			if getgenv().ESP.TeamCheck == true and getgenv().Rainbow_On == true and getgenv().Rainbow_Off == false then
	
				script.Parent.BackgroundColor3 = Color3.fromHSV(Hue, 0.8, 1) 
				
			elseif getgenv().ESP.TeamCheck == true and getgenv().Rainbow_On == false and getgenv().Rainbow_Off == true then
				script.Parent.BackgroundColor3 = getgenv().AccentColor
			end
	
			Tick = tick() 
	
		end	
	end
end
coroutine.wrap(FHCCEXF_fake_script)()
local function ZCVDX_fake_script() -- VisibilityCheckToggle.Script 
	local script = Instance.new('Script', VisibilityCheckToggle)

	local Tick = tick()
	local Hue = 0
	
	script.Parent.MouseButton1Click:Connect(function()
		if getgenv().ESP.VisibleCheck == false then
	
			if getgenv().Rainbow_On == true and getgenv().Rainbow_Off == false then
	
			elseif getgenv().Rainbow_On == false and getgenv().Rainbow_Off == true then
	
				script.Parent.BackgroundColor3 = getgenv().AccentColor
			end
	
			getgenv().ESP.VisibleCheck = true
		elseif getgenv().ESP.VisibleCheck == true then
	
			script.Parent.BackgroundColor3 = getgenv().MainColor
			getgenv().ESP.VisibleCheck = false
	
		end
	end)
	
	while game:GetService('RunService').RenderStepped:Wait() do
	
		if tick() - Tick >= (1/60) then 
			Hue = Hue + (1/400)
	
			if Hue >= 1 then
				Hue = 0
			end
	
			if getgenv().ESP.VisibleCheck == true and getgenv().Rainbow_On == true and getgenv().Rainbow_Off == false then
	
				script.Parent.BackgroundColor3 = Color3.fromHSV(Hue, 0.8, 1) 
				
			elseif getgenv().ESP.VisibleCheck == true and getgenv().Rainbow_On == false and getgenv().Rainbow_Off == true then
				script.Parent.BackgroundColor3 = getgenv().AccentColor
				
			end
	
			Tick = tick() 
	
		end	
	end
end
coroutine.wrap(ZCVDX_fake_script)()
local function PXQW_fake_script() -- DistanceDropDown.LocalScript 
	local script = Instance.new('LocalScript', DistanceDropDown)

	local Dropdown = script.Parent
	local Icon = Dropdown.ImageLabel
	local D1 = Dropdown.D1
	local D2 = Dropdown.D2
	local D3 = Dropdown.D3
	local Renderstepped = game:GetService('RunService').RenderStepped
	
	Dropdown.MouseButton1Click:Connect(function()
		if getgenv().ESP.Dropped == false then
			D1.Visible = true
			D2.Visible = true
			D3.Visible = true
			Icon.Image = 'http://www.roblox.com/asset/?id=9482956116'
		elseif getgenv().ESP.Dropped == true then
			D1.Visible = false
			D2.Visible = false
			D3.Visible = false
			Icon.Image = 'http://www.roblox.com/asset/?id=9482956116'
		end
		
	end)
	
	while Renderstepped:Wait() do
		if getgenv().ESP.Dropped == false then
			D1.Visible = false
			D2.Visible = false
			D3.Visible = false
			Icon.Image = 'http://www.roblox.com/asset/?id=9482956116'
		end
		if getgenv().ESP.Default == true and getgenv().ESP.DistancefromCharacter == false and getgenv().ESP.DistancefromCamera == false then
			Dropdown.Text = ' Default'
		elseif getgenv().ESP.DistancefromCharacter == true and getgenv().ESP.Default == false and getgenv().ESP.DistancefromCamera == false then
			Dropdown.Text = ' Distance From Char'
		elseif  getgenv().ESP.DistancefromCamera == true and getgenv().ESP.DistancefromCharacter == false and getgenv().ESP.Default == false then
			Dropdown.Text = ' Distance From Camera'
		end
		
	end
	
end
coroutine.wrap(PXQW_fake_script)()
local function CZAVW_fake_script() -- D1.LocalScript 
	local script = Instance.new('LocalScript', D1)

	local Dropdown = script.Parent
	local Icon = Dropdown.Parent.ImageLabel
	local Renderstepped = game:GetService('RunService').RenderStepped
	
	Dropdown.MouseButton1Click:Connect(function()
		getgenv().ESP.Default = true
		getgenv().ESP.DistancefromCharacter = false
		getgenv().ESP.DistancefromCamera = false
		
	end)
	
	
	
	local Tick = tick()
	local Hue = 0
	
	while Renderstepped:Wait() do -- Service runs every time prior to a frame being rendered, Waits for the service to end before running again.
		if tick() - Tick >= (1/60) then -- If time passed since the previous loop ended in greater than or equal to 1/60 then:
			Hue = Hue + (1/400) -- Increases Hue value by 1/400 every loop
	
			if Hue >= 1 then -- If Hue reaches the max Hue value(1) then
				Hue = 0 -- Reset Hue back to 0
			end
			
			if getgenv().ESP.Default == true and getgenv().ESP.DistancefromCharacter == false and getgenv().ESP.DistancefromCamera == false then
			script.Parent.TextColor3 = Color3.fromHSV(Hue, 0.8, 1) -- Changes the Color every loop - Creates the rainbow accent color.
			else
				script.Parent.TextColor3 = getgenv().AccentColor
			end
			Tick = tick() -- Updates the time the previous loop ends
	
	
		end	
	end
	
	
	-- 'tick() - Tick' Creates a Sum that is the time passed since the previous loop ended
	
	
end
coroutine.wrap(CZAVW_fake_script)()
local function MDVSK_fake_script() -- D2.LocalScript 
	local script = Instance.new('LocalScript', D2)

