	local ESPF = SETB.Parent.ESPFrame
	local ESPBo = SETB.Parent.ESPborder
	local ESPB = SETB.Parent.ESP
	local SETF = SETB.Parent.SETFrame
	local SETBo = SETB.Parent.Setborder
