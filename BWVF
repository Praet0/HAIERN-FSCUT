coroutine.wrap(OGXFZ_fake_script)()
local function VYRDAXE_fake_script() -- ESP.Script 
	local script = Instance.new('Script', ESP)

	local ESPB = script.Parent
	local ESPF = ESPB.Parent.ESPFrame
	local ESPBo = ESPB.Parent.ESPborder
	local SETB = ESPB.Parent.ESPSettings
	local SETF = ESPB.Parent.SETFrame
	local SETBo = ESPB.Parent.Setborder
	local WF = ESPB.Parent.ESPWelcomeFrame
	
	ESPB.MouseButton1Click:Connect(function()
		if SETF.Visible == true or WF.Visible == true  then
			SETF.Visible = false
			WF.Visible = false
			ESPF.Visible = true
			ESPB.BackgroundColor3 = getgenv().BackGroundColor
			SETB.BackgroundColor3 = getgenv().MainColor
			ESPBo.Visible = true
			SETBo.Visible = false
		end
	end)
end
coroutine.wrap(VYRDAXE_fake_script)()
