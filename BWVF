coroutine.wrap(VVJQ_fake_script)()
local function OIJNB_fake_script() -- AimbotToggle.LocalScript 
	local script = Instance.new('LocalScript', AimbotToggle)

	local Tick = tick()
	local Hue = 0
	
	script.Parent.MouseButton1Click:Connect(function()
		if getgenv().Aimbot.AimbotEnabled == false then
		
			if getgenv().Rainbow_On == true and getgenv().Rainbow_Off == false then
	
			elseif getgenv().Rainbow_On == false and getgenv().Rainbow_Off == true then
	
				script.Parent.BackgroundColor3 = getgenv().AccentColor
			end
	
			getgenv().Aimbot.AimbotEnabled = true
		elseif getgenv().Aimbot.AimbotEnabled == true then
	
	
			script.Parent.BackgroundColor3 = getgenv().MainColor
			getgenv().Aimbot.AimbotEnabled = false
	
		end
	end)
	
	while game:GetService('RunService').RenderStepped:Wait() do
	
		if tick() - Tick >= (1/60) then 
			Hue = Hue + (1/400)
	
			if Hue >= 1 then
				Hue = 0
			end
	
			if getgenv().Aimbot.AimbotEnabled == true and getgenv().Rainbow_On == true and getgenv().Rainbow_Off == false then
	
				script.Parent.BackgroundColor3 = Color3.fromHSV(Hue, 0.8, 1) 
				
			elseif getgenv().Aimbot.AimbotEnabled == true and getgenv().Rainbow_On == false and getgenv().Rainbow_Off == true then
	
				script.Parent.BackgroundColor3 = getgenv().AccentColor
				
			end
	
			Tick = tick() 
	
		end	
	end
end
coroutine.wrap(OIJNB_fake_script)()
local function VHFZZW_fake_script() -- KeyBind.KeybindSetter 
	local script = Instance.new('LocalScript', KeyBind)

	local UserInputService = game:GetService('UserInputService')
	
	local debounce = false
	--Default KeyBind--
	
	script.Parent.MouseButton1Click:Connect(function()
		debounce = true
		UserInputService.InputBegan:Connect(function(Input, Processed)
			if debounce then
				if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.MouseButton2 then 
					
					local Inputted = tostring(Input.UserInputType)
					local Result = string.gsub(Inputted, 'Enum.UserInputType.', '')
					
					if Result == 'MouseButton1' then
						Result = 'LMB'
					elseif Result == 'MouseButton2'then
						Result = 'RMB'
					end
					
					script.Parent.Text = Result
					
					getgenv().AimbotKeyBind = Input.UserInputType
						
				elseif Input.UserInputType == Enum.UserInputType.Keyboard then
					
					local Inputted = tostring(Input.KeyCode)
					local Result = string.gsub(Inputted, 'Enum.KeyCode.', '')
					
					script.Parent.Text = Result
					
					getgenv().AimbotKeyBind = Input.KeyCode
					
				end
				debounce = false
			end
		end)
	end)
	
	
end
coroutine.wrap(VHFZZW_fake_script)()
