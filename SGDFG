local function UTTXFK_fake_script() -- HeadShot_Slider.LocalScript 
	local script = Instance.new('LocalScript', HeadShot_Slider)

	--Variables-------------------------------------------------
	local UIS = game:GetService('UserInputService')
	local Slider = script.Parent
	local InnerSliderA = script.Parent.InnerSliderA
	local InnerSliderB = script.Parent.InnerSliderB
