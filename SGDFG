local function EODOSGM_fake_script() -- ESPSettings.Script 
	local script = Instance.new('Script', ESPSettings)

	
	
	
	
	SETB.MouseButton1Click:Connect(function()
			if ESPF.Visible == true or WF2.Visible == true  then
			SETF.Visible = true
			ESPF.Visible = false
			WF2.Visible = false
			ESPB.BackgroundColor3 = getgenv().MainColor
			SETB.BackgroundColor3 = getgenv().BackGroundColor
			ESPBo.Visible = false
			SETBo.Visible = true
		end
