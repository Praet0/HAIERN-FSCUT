local function FHVHJ_fake_script() -- ESP.Script 
	local script = Instance.new('Script', ESP)

	ESPB = script.Parent
	ESPF = ESPB.Parent.ESPFrame
	ESPBo = ESPB.Parent.ESPborder
	SETB = ESPB.Parent.ESPSettings
	SETF = ESPB.Parent.SETFrame
	SETBo = ESPB.Parent.Setborder
	WF2 = ESPB.Parent.ESPWelcomeFrame
	--WFI = WF.ESPINFO
	
	ESPB.MouseButton1Click:Connect(function()
		if SETF.Visible == true or WF2.Visible == true  then
			SETF.Visible = false
			WF2.Visible = false
			ESPF.Visible = true
			ESPB.BackgroundColor3 = getgenv().BackGroundColor
			SETB.BackgroundColor3 = getgenv().MainColor
			ESPBo.Visible = true
			SETBo.Visible = false
		end
	end)
end
