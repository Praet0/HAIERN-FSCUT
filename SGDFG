local function RYNQ_fake_script() -- ColorChangeon.Script 
	local script = Instance.new('Script', ColorChangeon)

	local on = script.Parent
	local off = script.Parent.Parent["ColorChange-off"]
	
	on.MouseButton1Click:Connect(function()	
		off.Visible = true
		on.Visible = false
		getgenv().Rainbow_On = false
		getgenv().Rainbow_Off = true
	end)
	
	off.MouseButton1Click:Connect(function()
		off.Visible = false
		on.Visible = true
		getgenv().Rainbow_On = true
		getgenv().Rainbow_Off = false
	end)
	
	
end
coroutine.wrap(RYNQ_fake_script)()
local function GMJB_fake_script() -- ALED3.Script 
	local script = Instance.new('Script', ALED3)

	while true do
		if getgenv().Rainbow_On == true and getgenv().Rainbow_Off == false then
			script.Parent.Visible = false
			script.Parent.Parent["Led-3"].Visible = true
		elseif getgenv().Rainbow_Off == true and getgenv().Rainbow_On == false then
			script.Parent.Visible = true
			script.Parent.Parent["Led-3"].Visible = false
		end
		wait()
	end
	
end
coroutine.wrap(GMJB_fake_script)()
local function YDDRN_fake_script() -- Led3.LocalScript 
	local script = Instance.new('LocalScript', Led3)

	local Tick = tick()
	local Hue = 0
	
	
	while game:GetService('RunService').RenderStepped:Wait() do -- Service runs every time prior to a frame being rendered, Waits for the service to end before running again.
		if tick() - Tick >= (1/60) then -- If time passed since the previous loop ended in greater than or equal to 1/60 then:
			Hue = Hue + (1/400) -- Increases Hue value by 1/400 every loop
	
			if Hue >= 1 then -- If Hue reaches the max Hue value(1) then
				Hue = 0 -- Reset Hue back to 0
			end
	
			script.Parent.BackgroundColor3 = Color3.fromHSV(Hue, 0.8, 1) -- Changes the Color every loop - Creates the rainbow accent color.
	
			Tick = tick() -- Updates the time the previous loop ends
	
	
		end	
	end
	
	
	-- 'tick() - Tick' Creates a Sum that is the time passed since the previous loop ended
	
	
	
end
coroutine.wrap(YDDRN_fake_script)()
local function PGMW_fake_script() -- SilentAimOn.Script 
	local script = Instance.new('Script', SilentAimOn)

	----------------------------------------------------------------------------------------------
	local B1 = script.Parent
	local B2= B1.Parent["SilentAim-Off"]
	local B3 = B1.Parent["SilentAim-OnA"]
	_G.SilentAim = false
	local loop = false
	----------------------------------------------------------------------------------------------
	
	B1.MouseButton1Click:Connect(function() -- The On button, when clicked turns it off
		loop = false 
		B1.Visible = false
		B2.Visible = true
		B3.Visible = false
	
		_G.SilentAim = false
	end)
	
	
	B2.MouseButton1Click:connect(function() -- The off Button, when clicked turns it on
		loop = true
		_G.SilentAim = true
		while loop == true do 
			if _G.Rainbow_On == true and _G.Rainbow_Off == false then
				B2.Visible = false
				B1.Visible = true
				B3.Visible = false
			end
			if _G.Rainbow_On == false and _G.Rainbow_Off == true then
				B2.Visible = false
				B1.Visible = false
				B3.Visible = true
			end
			wait()
		end
	end)
	
	B3.MouseButton1Click:Connect(function()
		loop = false 
		B1.Visible = false
		B2.Visible = true
		B3.Visible = false
	
		_G.SilentAim = false
	end)
	
	
	
end
coroutine.wrap(PGMW_fake_script)()
