local function ECKHPS_fake_script() -- InnerSliderB.Script 
	local script = Instance.new('Script', InnerSliderB)

	local B = script.Parent
	local A = script.Parent.Parent.InnerSliderA
	local RenderStepped = game:GetService('RunService').RenderStepped
	B.Visible = true
	A.Visible = false
	
	
	local loop = true
	while loop == true do 
		if _G.Rainbow_On == true and _G.Rainbow_Off == false then
			B.Visible = true
			A.Visible = false
		end
		if _G.Rainbow_On == false and _G.Rainbow_Off == true then
			B.Visible = false
			A.Visible = true
		end
		wait()
	end
	
end
