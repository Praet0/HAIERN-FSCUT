-- Scripts:

local function FQLK_fake_script() -- ScreenGui.MouseScript 
	local script = Instance.new('LocalScript', ScreenGui)

	local Mouse = game.Players.LocalPlayer:GetMouse()
	
	Mouse.Icon = 'http://www.roblox.com/asset/?id=5025970007'
end
coroutine.wrap(FQLK_fake_script)()
local function UWXVVAM_fake_script() -- OpenButton.Script 
	local script = Instance.new('Script', OpenButton)

	local OpenButton = script.Parent
	local UI = script.Parent.Parent.MainLoginFrame
	
	local debounce = false
	
	OpenButton.MouseButton1Click:Connect(function()
		if not debounce then
		debounce = true
		UI:TweenPosition(
			UDim2.new(0.4, 0,0.195, 0),
			'Out',
			'Sine',
			3,
			false,
			nil
			)
		end
		script.Parent:Destroy()
	end)
end
coroutine.wrap(UWXVVAM_fake_script)()
local function VTVN_fake_script() -- LoginButton.LocalScript 
	local script = Instance.new('LocalScript', LoginButton)

	GUI = script.Parent.Parent.Parent.BBFrame
	local debounce = false
	
	Username = script.Parent.Parent.UsernameBox
	Password = script.Parent.Parent.PasswordBox
	
	script.Parent.MouseButton1Click:Connect(function()
		print(1)
		if not debounce then
			debounce = true
			if  Username.Text == 'Prae' and Password.Text == '3082' or Username.Text == 'Aty' and Password.Text == '5970' then
				script.Parent.Text = 'Loading...'
				wait(3)
				
				GUI:TweenPosition(
					UDim2.new(0.3, 0,-0.03, 0),
					'Out',
					'Sine',
					3,
					false,
					nil	
				)
				script.Parent.Parent:Destroy()
			else
				script.Parent.Text = 'Error!'
				wait(3)
				script.Parent.Text = 'Login'
			end
			wait()
			debounce = false
			end
	end)
end
coroutine.wrap(VTVN_fake_script)()
local function HUTZ_fake_script() -- BBFrame.KeyBind 
	local script = Instance.new('LocalScript', BBFrame)

	local UserInputService = game:GetService('UserInputService')
	
	
	UserInputService.InputBegan:Connect(function(Input, gameProcessedEvent)
		if Input.KeyCode == Enum.KeyCode.LeftAlt then
			script.Parent.Active = not script.Parent.Active
			script.Parent.Visible = not script.Parent.Visible
	
		end
	end)
end
coroutine.wrap(HUTZ_fake_script)()
local function MFAAECA_fake_script() -- BBFrame.DragScript 
	local script = Instance.new('LocalScript', BBFrame)

	local UIS = game:GetService('UserInputService')
	local Players = game:GetService('Players')
	local Mouse = Players.LocalPlayer:GetMouse()
	local RenderStepped = game:GetService('RunService').RenderStepped
	
	script.Parent.InputBegan:Connect(function(Input)
		if Input.UserInputType == Enum.UserInputType.MouseButton1 then
			local OrgPos = Vector2.new(
				Mouse.X - script.Parent.AbsolutePosition.X,
				Mouse.Y - script.Parent.AbsolutePosition.Y
			)
			local OrgAnc_X = script.Parent.Size.X.Offset * script.Parent.AnchorPoint.X
			local OrgAnc_Y = script.Parent.Size.Y.Offset * script.Parent.AnchorPoint.Y
			
			while UIS:IsMouseButtonPressed(Enum.UserInputType.MouseButton1) do
				
				script.Parent.Position = UDim2.new(
					0,
					Mouse.X - OrgPos.X + OrgAnc_X,
					0,
					Mouse.Y - OrgPos.Y + OrgAnc_Y
				)
				
				RenderStepped:Wait()
			end
			if script.Parent.AbsolutePosition.Y <= -58  then
				script.Parent.Position = UDim2.new(script.Parent.Position.X.Scale, script.Parent.Position.X.Offset , script.Parent.Position.Y.Scale, script.Parent.Position.Y.Offset + 30)
			end
		end
	end)
end
coroutine.wrap(MFAAECA_fake_script)()
local function VYZDYP_fake_script() -- TeamCheckOff.Script 
	local script = Instance.new('Script', TeamCheckOff)

	local Toggle = false
	local Tick = tick()
	local Hue = 0
	
	script.Parent.MouseButton1Click:Connect(function()
		if Toggle == false then
			Toggle = true
			if getgenv().Rainbow_On == true and getgenv().Rainbow_Off == false then
	
			elseif getgenv().Rainbow_On == false and getgenv().Rainbow_Off == true then
	
				script.Parent.BackgroundColor3 = getgenv().AccentColor
			end
	
			getgenv().ESP.TeamCheck = true
		elseif Toggle == true then
	
			Toggle = false
			script.Parent.BackgroundColor3 = getgenv().MainColor
			getgenv().ESP.TeamCheck = false
	
		end
	end)
	
	while game:GetService('RunService').RenderStepped:Wait() do
	
		if tick() - Tick >= (1/60) then 
			Hue = Hue + (1/400)
	
			if Hue >= 1 then
				Hue = 0
			end
	
			if Toggle == true and getgenv().Rainbow_On == true and getgenv().Rainbow_Off == false then
	
				script.Parent.BackgroundColor3 = Color3.fromHSV(Hue, 0.8, 1) 
	
			end
	
			Tick = tick() 
	
		end	
	end
end
coroutine.wrap(VYZDYP_fake_script)()
